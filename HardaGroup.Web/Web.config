<?xml version="1.0" encoding="utf-8"?>
<!--
  有关如何配置 ASP.NET 应用程序的详细信息，请访问
  http://go.microsoft.com/fwlink/?LinkId=301880
  -->
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    
    <section name="RewriterConfig" type="URLRewriter.Config.RewriterConfigSerializerSectionHandler, URLRewriter" />
    <!-- Add log4net config section-->
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  
  </configSections>

  <!--log4net 日志-->
  <log4net debug="true">
    <!--MVC日志配置-->
    <appender name="MVCLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <!--定义文件存放位置-->
      <file value="logs\MVC\Exception" />
      <!--多线程时采用最小锁定-->
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
      <!--日期的格式，每天换一个文件记录，如不设置则永远只记录一天的日志，需设置-->
      <datePattern value="-yyyyMMdd.'log'" />
      <!--是否追加到文件,默认为true，通常无需设置-->
      <appendToFile value="true" />
      <!--变换的形式为日期，这种情况下每天只有一个日志-->
      <!--此时MaxSizeRollBackups和maximumFileSize的节点设置没有意义-->
      <!--<rollingStyle value="Date"/>-->
      <!--文件以那种方式变换文件名：data日期/Size大小/Composite同时按照日期和大小-->
      <rollingStyle value="Composite" />

      <!--变换的形式为日志大小-->
      <!--这种情况下MaxSizeRollBackups和maximumFileSize的节点设置才有意义-->
      <!--<RollingStyle value="Size"/>-->
      <!--每天记录的日志文件个数，与maximumFileSize配合使用-->
      <MaxSizeRollBackups value="10" />
      <!--每个日志文件的最大大小-->
      <!--可用的单位:KB|MB|GB-->
      <!--不要使用小数,否则会一直写入当前日志-->
      <maximumFileSize value="10MB" />
      <!--StaticLogFileName的值如果为true，
      则当前日志文件(相对于历史日志文件而言)的文件名只取File参数。
      如果为false，则文件名取File+DatePattern。-->
      <staticLogFileName value="false" />

      <layout type="log4net.Layout.PatternLayout">
        <!--输出格式-->
        <!--样例：2008-03-26 13:42:32,111 [10] INFO  Log4NetDemo.MainClass [(null)] - info-->
        <conversionPattern value="%n%n%n%date 线程:[%thread] 级别：%-5level - 异常：%message%n%n" />

      </layout>
    </appender>

    <!--控制级别，由低到高: ALL|DEBUG|INFO|WARN|ERROR|FATAL|OFF-->
    <!--比如定义级别为INFO，则INFO级别向下的级别，比如DEBUG日志将不会被记录-->
    <!--如果没有定义LEVEL的值，则缺省为DEBUG-->
    <root>
      <level value="ERROR" />
    </root>
    <logger name="MVCLog" additivity="false">
      <level value="ERROR" />
      <appender-ref ref="MVCLogFileAppender" />
    </logger>

  </log4net>

  <connectionStrings>
    <add name="DefaultConnection" connectionString="Server = localhost\SQLEXPRESS;Database=HardaGroupDB;User ID = HardaAdmin;Password = Admin_1234;Trusted_Connection = False;" providerName="System.Data.SqlClient" />

    
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />

    <add key="UrlRewriteExtension" value=".html" />
  </appSettings>
  <system.web>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.5">
      <!-- 加上此节点，保证原本就是.html类型的文件能正常访问 -->
      <buildProviders>
        <add extension=".html" type="System.Web.Compilation.PageBuildProvider" />
      </buildProviders>
    </compilation>
    
    <httpRuntime targetFramework="4.5" />

    <!--On 表示在本地和远程用户都会看到自定义错误信息。
    Off 禁用自定义错误信息，本地和远程用户都会看到详细的错误信息。
    RemoteOnly 表示本地用户将看到详细错误信息，而远程用户将会看到自定义错误信息。-->
    <!--<customErrors mode="Off" defaultRedirect="/Home/Error500">
      <error statusCode="404" redirect="/Home/Error404" />

    </customErrors>-->
    <customErrors mode="Off" />
    
  </system.web>
  <system.webServer>
    <!--此301跳转-->
    <rewrite>
      <rules>
        <rule name="Redirect to www">
          <match url=".*" />
          <conditions>
            <add input="{HTTP_HOST}" pattern="^ahllen.net" />
          </conditions>
          <action type="Redirect" url="http://www.ahllen.net/{R:0}" redirectType="Permanent" />
        </rule>
      </rules>
    </rewrite>
    
    <modules>
      
      <!--URL重写-->
      <add type="URLRewriter.ModuleRewriter, URLRewriter" name="ModuleRewriter" />
      <!--多语言全球化-->
      <add name="CultureAwareHttpModule" type="HardaGroup.Web.Globalization.CultureAwareHttpModule,HardaGroup.Web" />

      <remove name="FormsAuthentication" />
    </modules>
    <handlers>
      <!--如果改变此处的path值，则要修改appSettings中相应的配置-->
      <add name="Html-Integrate" path="*.html" verb="GET,HEAD,POST,DEBUG" type="System.Web.UI.PageHandlerFactory" resourceType="Unspecified" requireAccess="Script" preCondition="integratedMode" />

    </handlers>
    
  </system.webServer>
  
  <!--URL重写配置-->
  <RewriterConfig>
    <Rules>
      <!--关于华尔达 -->
      <RewriterRule>
        <LookFor>~/about.html</LookFor>
        <SendTo>~/About/Index</SendTo>
      </RewriterRule>
      <!--匹配带有国际化的URL-->
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/about.html</LookFor>
        <SendTo>~$1/About/Index</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/about/([A-Za-z-]+).html</LookFor>
        <SendTo>~/About/Index?typecode=$1</SendTo>
      </RewriterRule>
      <!--匹配带有国际化的URL-->
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/about/([A-Za-z-]+).html</LookFor>
        <SendTo>~$1/About/Index?typecode=$2</SendTo>
      </RewriterRule>
      
      <!--智能制造-->
      <RewriterRule>
        <LookFor>~/advanced-manufacturing.html</LookFor>
        <SendTo>~/AdvancedManufacturing/Index</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/advanced-manufacturing.html</LookFor>
        <SendTo>~$1/AdvancedManufacturing/Index</SendTo>
      </RewriterRule>
      <!--解决方案-->
      <RewriterRule>
        <LookFor>~/solutions/parts-and-modules.html</LookFor>
        <SendTo>~/Solutions/PartsAndModules</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/solutions/parts-and-modules.html</LookFor>
        <SendTo>~$1/Solutions/PartsAndModules</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/solutions/research-and-development.html</LookFor>
        <SendTo>~/Solutions/ResearchAndDevelopment</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/solutions/research-and-development.html</LookFor>
        <SendTo>~$1/Solutions/ResearchAndDevelopment</SendTo>
      </RewriterRule>
      
      <!--创新创业-->
      <RewriterRule>
        <LookFor>~/venture-support.html</LookFor>
        <SendTo>~/VentureSupport/Index</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/venture-support.html</LookFor>
        <SendTo>~$1/VentureSupport/Index</SendTo>
      </RewriterRule>
      <!--媒体资讯-->
      <RewriterRule>
        <LookFor>~/press.html</LookFor>
        <SendTo>~/News/index</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/press.html</LookFor>
        <SendTo>~$1/News/index</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/press/([A-Za-z-]+).html</LookFor>
        <SendTo>~/News/Index?typecode=$1</SendTo>
      </RewriterRule>
      <!--匹配带有国际化的URL-->
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/press/([A-Za-z-]+).html</LookFor>
        <SendTo>~$1/News/Index?typecode=$2</SendTo>
      </RewriterRule>
      <!--详细-->
      <RewriterRule>
        <LookFor>~/press/([A-Za-z-]+)/([A-Za-z0-9-]+).html</LookFor>
        <SendTo>~/News/ShowDetail?typecode=$1&amp;newsUrlId=$2</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/press/([A-Za-z-]+)/([A-Za-z0-9-]+).html</LookFor>
        <SendTo>~$1/News/ShowDetail?typecode=$2&amp;newsUrlId=$3</SendTo>
      </RewriterRule>
      <!--加入我们-->
      <RewriterRule>
        <LookFor>~/join-us.html</LookFor>
        <SendTo>~/Home/joinus</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/join-us.html</LookFor>
        <SendTo>~$1/home/joinus</SendTo>
      </RewriterRule>

      <!--联系我们 -->
      <RewriterRule>
        <LookFor>~/contact-us.html</LookFor>
        <SendTo>~/Home/ContactUs</SendTo>
      </RewriterRule>
      <!--匹配带有国际化的URL-->
      <RewriterRule>
        <LookFor>~([A-Za-z0-9-]+)/contact-us.html</LookFor>
        <SendTo>~$1/Home/ContactUs</SendTo>
      </RewriterRule>
      
      
      <!-- 后台管理URL -->
      <RewriterRule>
        <LookFor>~/manage.html</LookFor>
        <SendTo>~/manage/index</SendTo>
      </RewriterRule>
      
      <RewriterRule>
        <LookFor>~/manage/([A-Za-z]+).html</LookFor>
        <SendTo>~/manage/$1</SendTo>
      </RewriterRule>
      <!--背景图片管理-->
      <RewriterRule>
        <LookFor>~/manage/background-([A-Za-z0-9-]+).html</LookFor>
        <SendTo>~/manage/backgroundimage?type=$1</SendTo>
      </RewriterRule>
      <!--智能制造-->
      <RewriterRule>
        <LookFor>~/manage/manufacturing/([A-Za-z0-9-]+).html</LookFor>
        <SendTo>~/manage/manufacturing$1</SendTo>
      </RewriterRule>
      <!--媒体资讯管理-->
      <RewriterRule>
        <LookFor>~/manage/news-scope.html</LookFor>
        <SendTo>~/manage/NewsScope</SendTo>
      </RewriterRule>
      <RewriterRule>
        <LookFor>~/manage/news.html</LookFor>
        <SendTo>~/manage/News</SendTo>
      </RewriterRule>
      <!--登录-->
      <RewriterRule>
        <LookFor>~/login.html?(.+)</LookFor>
        <SendTo>~/account/login?$1</SendTo>
      </RewriterRule>
    </Rules>
  </RewriterConfig>
  
